<?xml version="1.0" encoding="UTF-8"?>
<glade-catalog name="edosuui" depends="gtk+" domain="edosu">
  <glade-widget-classes>
    <glade-widget-class name="EdosuCloseLabel" generic-name="edosu_close_label" title="CloseLabel">
    <properties>
        <property translatable="True" multiline="True" id="label" default="label" custom-layout="True" />
    </properties>
    </glade-widget-class>

    <glade-widget-class name="EdosuPopover" generic-name="edosu_editor_popover" title="EditorPopover"/>
    <glade-widget-class name="EdosuWindow" generic-name="edosu_editor_window" title="EditorWindow"/>
  </glade-widget-classes>

  <glade-widget-group name="edosu" title="Edosu">
    <glade-widget-class-ref name="EdosuCLoseLabel"/>
    <glade-widget-class-ref name="EdosuPopover"/>
    <glade-widget-class-ref name="EdosuWindow"/>
  </glade-widget-group>
</glade-catalog>
